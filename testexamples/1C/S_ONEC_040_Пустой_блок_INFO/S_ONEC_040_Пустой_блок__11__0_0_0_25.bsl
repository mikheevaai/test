    // S_ONEC_040 Пустой блок x1
    // S_ONEC_044 Использование функции «ТекущаяДата» x1
    ИначеЕсли ДеньНедели(ТекущаяДата()) = 7 Тогда
    // S_ONEC_040 Пустой блок x1
    Иначе
    КонецЕсли;

    // S_ONEC_061 Стиль написания ключевых слов x1
    Для ТекДень = 1 по ПоследнийДеньМесяца Цикл
        Состояние("Обрабатывается день: "+ ТекДень);
